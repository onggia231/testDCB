<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
    http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <absolute-ordering>
        <others />
        <name>lib</name>
    </absolute-ordering>

    <factory>
        <exception-handler-factory>
            com.faplib.lib.TSExceptionHandler
        </exception-handler-factory>
    </factory>

    <lifecycle>
        <phase-listener>
            com.faplib.admin.security.UserAuthListener
        </phase-listener>
    </lifecycle>

    <managed-bean>
        <managed-bean-name>centerProcess</managed-bean-name>
        <managed-bean-class>com.faplib.lib.admin.gui.CenterProcess</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>accessTimeController</managed-bean-name>
        <managed-bean-class>com.faplib.lib.admin.gui.AccessTimeController</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>addressController</managed-bean-name>
        <managed-bean-class>com.faplib.lib.admin.gui.AddressController</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>filePropertyController</managed-bean-name>
        <managed-bean-class>com.faplib.lib.admin.gui.FilePropertyController</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>logAccessController</managed-bean-name>
        <managed-bean-class>com.faplib.lib.admin.gui.LogAccessController</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>logChangeController</managed-bean-name>
        <managed-bean-class>com.faplib.lib.admin.gui.LogChangeController</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>mainController</managed-bean-name>
        <managed-bean-class>com.faplib.lib.admin.gui.MainController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>menuController</managed-bean-name>
        <managed-bean-class>com.faplib.lib.admin.gui.MenuController</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>policyController</managed-bean-name>
        <managed-bean-class>com.faplib.lib.admin.gui.PolicyController</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>systemGroupController</managed-bean-name>
        <managed-bean-class>com.faplib.lib.admin.gui.SystemGroupController</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>systemLogController</managed-bean-name>
        <managed-bean-class>com.faplib.lib.admin.gui.SystemLogController</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>systemModuleController</managed-bean-name>
        <managed-bean-class>com.faplib.lib.admin.gui.SystemModuleController</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>mobileModuleController</managed-bean-name>
        <managed-bean-class>com.faplib.lib.admin.gui.MobileModuleController</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>systemUserController</managed-bean-name>
        <managed-bean-class>com.faplib.lib.admin.gui.SystemUserController</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>birtReportController</managed-bean-name>
        <managed-bean-class>com.faplib.lib.admin.gui.BirtReportController</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>reportViewerController</managed-bean-name>
        <managed-bean-class>com.faplib.lib.admin.gui.ReportViewerController</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>mngReportController</managed-bean-name>
        <managed-bean-class>com.faplib.lib.admin.gui.MngReportController</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>mngAppsController</managed-bean-name>
        <managed-bean-class>com.faplib.lib.admin.gui.MngAppsController</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>adminUser</managed-bean-name>
        <managed-bean-class>com.faplib.admin.security.AdminUser</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>centralAuthService</managed-bean-name>
        <managed-bean-class>com.faplib.admin.security.CentralAuthService</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>exceptionHandler</managed-bean-name>
        <managed-bean-class>com.faplib.admin.security.ExceptionHandler</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>systemConfig</managed-bean-name>
        <managed-bean-class>com.faplib.lib.SystemConfig</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
</faces-config>